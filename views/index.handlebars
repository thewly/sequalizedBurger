<div class="container-fluid">
  <div class="container">
    <div class="row">
      <div class="col-sm-4">

      </div>
      <div class="col-sm-4">
        <h1 class="text-center">Burgers Tracker App!</h1>
        <img class="rounded mx-auto d-block" height="300px" width="100%" src="https://video-images.vice.com/articles/5ac53328a1e6180008345f98/lede/1522873279699-DZsqULvXUAIyJBw.jpeg?crop=1xw%3A0.6832740213523132xh%3Bcenter%2Ccenter&resize=650%3A*&output-quality=55">
      </div>
      <div class="col-sm-4">

      </div>
    </div>
  </div>
  <div class="row" >
    <div class="col-md-6" id="Eat">

  <h2>Burgers I want to eat.</h2>
      <ul>
        {{#each burgers}}
        {{#unless devoured}}
        <li>
	{{name}}

	<button class="change-eaten" data-id="{{id}}" data-neweaten="true">
		Eat It
	</button>

	
</li>

        {{/unless}}
        {{/each}}
      </ul>
    </div>
    <div class="col-md-6" id="Completed">

      <h2>Devoured Burgers</h2>

      <ul>
        {{#each burgers}}
        {{#if devoured}}
       <li id="li2">
	{{name}}
	
</li>

        {{/if}}
        {{/each}}
      </ul>
    </div>
  </div>
  <div class="row" id="AddBurger">
    <div class="col-md-12">

      <h2>Add a Burger</h2>
      <form class="create-form">

        <div class="form-group">
          <label for="ca">Burger Name:</label>
          <input type="text" id="ca" name="name">
        </div>

        <button type="submit">Add Burger</button>
      </form>
    </div>
  </div>
</div>